diff -ruN linux-5.4.86-orig/kernel/find_roots.c linux-5.4.86-dev/kernel/find_roots.c
--- linux-5.4.86-orig/kernel/find_roots.c	1970-01-01 00:00:00.000000000 +0000
+++ linux-5.4.86-dev/kernel/find_roots.c	2021-03-21 11:17:12.749194452 +0000
@@ -0,0 +1,29 @@
+#include <linux/kernel.h>
+#include <linux/syscalls.h>
+#include <linux/sched.h>
+#define INIT_PID 1
+
+SYSCALL_DEFINE0(find_roots){
+
+	struct task_struct *current_task = current;
+	struct task_struct *task;
+	char comm[TASK_COMM_LEN];	
+
+	printk(KERN_INFO "find_roots system call called by proccess %d", current_task->pid);
+
+
+	for(task = current_task; task->pid != INIT_PID; task = task->real_parent){
+	
+		get_task_comm(comm, task);
+
+		printk(KERN_INFO "id:%d, name:%s\n", task->pid, comm);	
+
+	}
+	
+	get_task_comm(comm, task);
+
+	printk(KERN_INFO "id:%d, name:%s\n", task->pid, comm);
+		
+
+	return 0;
+}
